{
  "name" : "Photoverse",
  "version" : "1.0",
  "description" : "Get playlists that could be associated with an image!",
  "background" : { "scripts": ["imageinfo/jquery-2.2.0.min.js", "background.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "imageinfo-16.png",
    "48" : "imageinfo-48.png",
    "128" : "imageinfo-128.png"
  },
  "manifest_version": 2
}
